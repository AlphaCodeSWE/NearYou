        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "ClickHouse"
          },
          "format": 1,
          "queryType": "sql",
          "rawSql": "SELECT \n    CASE \n        WHEN discount_percent < 10 THEN '< 10%'\n        WHEN discount_percent < 20 THEN '10-19%'\n        WHEN discount_percent < 30 THEN '20-29%'\n        WHEN discount_percent < 40 THEN '30-39%'\n        ELSE '40%+'\n    END as sconto_range,\n    ROUND(countIf(v.offer_accepted) / count() * 100, 1) as conversion_rate,\n    COUNT(*) as total_visits\nFROM nearyou.user_visits v\nJOIN (\n    SELECT shop_id, discount_percent \n    FROM postgres.near_you_shops.offers o\n    WHERE o.is_active = true\n) o ON v.shop_id = o.shop_id\nWHERE v.visit_start_time >= now() - INTERVAL 7 DAY\nGROUP BY sconto_range\nORDER BY sconto_range",
          "refId": "A"
        }
      ],
      "title": "Conversione per Livello Sconto",
      "type": "barchart"
