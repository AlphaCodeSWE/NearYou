          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH shop_revenue AS (\n    SELECT \n        shop_id,\n        shop_name,\n        SUM(estimated_spending) as revenue,\n        COUNT(*) as total_visits\n    FROM clickhouse('clickhouse-server:9000', 'nearyou', 'user_visits', 'default', 'pwe@123@l@')\n    WHERE visit_start_time >= NOW() - INTERVAL '7 days'\n    GROUP BY shop_id, shop_name\n)\nSELECT \n    s.shop_id,\n    s.shop_name,\n    s.category,\n    ST_Y(s.geom) as lat,\n    ST_X(s.geom) as lon,\n    COALESCE(sr.revenue, 0) as revenue,\n    COALESCE(sr.total_visits, 0) as visits\nFROM shops s\nLEFT JOIN shop_revenue sr ON s.shop_id = sr.shop_id\nWHERE s.geom IS NOT NULL\nORDER BY sr.revenue DESC NULLS LAST\nLIMIT 200",
          "refId": "A"
        }
